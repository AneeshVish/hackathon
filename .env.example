# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/patient_ml_db
REDIS_URL=redis://localhost:6379/0

# MLflow Configuration
MLFLOW_TRACKING_URI=http://localhost:5000
MLFLOW_S3_ENDPOINT_URL=http://localhost:9000
AWS_ACCESS_KEY_ID=minioadmin
AWS_SECRET_ACCESS_KEY=minioadmin

# API Configuration
API_SECRET_KEY=your-secret-key-here-change-in-production
API_ALGORITHM=HS256
API_ACCESS_TOKEN_EXPIRE_MINUTES=30

# Security
ENCRYPTION_KEY=your-encryption-key-here-32-chars
HIPAA_AUDIT_LOG_LEVEL=INFO

# Feature Store
FEAST_REPO_PATH=./feature_store

# Model Configuration
DEFAULT_MODEL_VERSION=latest
PREDICTION_THRESHOLD_HIGH=0.30
PREDICTION_THRESHOLD_MEDIUM=0.10

# Monitoring
PROMETHEUS_PORT=8000
LOG_LEVEL=INFO

# External APIs (Optional)
FHIR_BASE_URL=https://your-fhir-server.com/fhir
FHIR_CLIENT_ID=your-client-id
FHIR_CLIENT_SECRET=your-client-secret

# Development
DEBUG=true
ENVIRONMENT=development
